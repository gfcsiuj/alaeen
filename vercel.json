{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/manifest.json",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=0"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "public, max-age=0"
      },
      "dest": "/sw.js"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=0"
      },
      "dest": "/index.html"
    }
  ]
}